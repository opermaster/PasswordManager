<Window x:Class="PasswordManagerU.EdittingWindows.SRDelWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PasswordManagerU.EdittingWindows"
        mc:Ignorable="d"
        Title="Delete service" Height="200" Width="300"  WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../Styles/BasicStyle.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <StackPanel>
        <Label Margin="10 10 10 0">Service Id:</Label>
        <TextBox x:Name="Service_Id_Box" Margin="10" MaxLength="30" PreviewTextInput="Data_Id_Box_PreviewTextInput"></TextBox>
        <Button Margin="10" Width="70" Click="Accept_Click">Delete</Button>
    </StackPanel>
</Window>
